#!/bin/sh
xrandr --output eDP-1 --primary --mode 3840x2400 --pos 640x2880 --rotate normal --output HDMI-2 --off --output DP-3 --off --output DP-4 --off --output DP-5 --off --output DP-6 --off --output DP-1-0 --off --output DP-1-1 --off --output HDMI-1-0 --off --output DP-1-2 --mode 5120x2880 --pos 0x0 --rotate normal --output DP-1-3 --off
